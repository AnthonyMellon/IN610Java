public class BankAccount {
	
	private double balance;
	
	public BankAccount()
	{
		balance = 0;
	}
	
	public int getBalance()
	{
		return balance;
	}
	
	public void deposit(int i)
	{
		balance += i;
	}
	
	public void withdraw(int i)
	{
		final double overdraftPenelty = 5; 
		
		balance -= i;
		if(balance < 0)
		{
			balance -= overdraftPenelty;
		}
	}
	
	
}
