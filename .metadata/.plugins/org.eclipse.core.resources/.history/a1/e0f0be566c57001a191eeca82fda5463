package pbox;

import static org.junit.jupiter.api.Assertions.*;

public class BoxTest {

	private Box box1;
	private Box box2;
	private Box box3;
	private Box box4;
	
	@Test
	void getMethods()
	{
		box1 = new Box(1, 2, 3);
		box2 = new Box(4, 5);
		box3 = new Box(6);
		box4 = new Box(0);
		
		assertEquals(6, box1.calcVolume());
	}
}
